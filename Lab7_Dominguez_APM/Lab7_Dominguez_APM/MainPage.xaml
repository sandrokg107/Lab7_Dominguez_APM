<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Lab7_Dominguez_APM"
             x:Class="Lab7_Dominguez_APM.MainPage">

    <ScrollView>
        <StackLayout>

            <Button x:Name="Item1" Text="Explicit"/>
            <Button x:Name="Item2" Text="Explicit Code"/>
            <Button x:Name="Item3" Text="Explicit Control"/>
            <Button x:Name="Item4" Text="Explicit Control Code"/>
            <Button x:Name="Item5" Text="Implicit"/>
            <Button x:Name="Item6" Text="Implicit Code"/>
            <Button x:Name="Item7" Text="Implicit Control"/>

            <Button x:Name="Item9" Text="Global"/>
